emplate>\n  <b-container class=\"bv-example-row mt-3\">\n    <b-row>\n      <b-col>\n        <b-alert show><h3>글목록</h3></b-alert>\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col class=\"text-right\">\n        <b-button variant=\"outline-primary\" @click=\"moveWrite()\"\n          >글쓰기</b-button\n        >\n      </b-col>\n    </b-row>\n    <b-row>\n      <b-col v-if=\"articles.length\">\n        <b-table-simple hover responsive>\n          <b-thead head-variant=\"dark\">\n            <b-tr>\n              <b-th>글번호</b-th>\n              <b-th>제목</b-th>\n              <b-th>조회수</b-th>\n              <b-th>작성자</b-th>\n              <b-th>작성일</b-th>\n            </b-tr>\n          </b-thead>\n          <tbody>\n            <!-- 하위 component인 ListRow에 데이터 전달(props) -->\n            <notice-list-row\n              v-for=\"(article, index) in articles\"\n              :key=\"index\"\n              v-bind=\"article\"\n            />\n          </tbody>\n        </b-table-simple>\n      </b-col>\n      <!-- <b-col v-else class=\"text-center\">도서 목록이 없습니다.</b-col> -->\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport NoticeListRow from \"@/components/notice/child/NoticeListRow\";\nimport { listArticle } from \"@/api/notice.js\";\n\nexport default {\n  name: \"NoticeList\",\n  components: {\n    NoticeListRow,\n  },\n  data() {\n    return {\n      articles: [],\n    };\n  },\n  created() {\n    let param = {\n      pg: 1,\n      spp: 20,\n      key: null,\n      word: null,\n    };\n    listArticle(\n      param,\n      (response) => {\n        this.articles = response.data;\n      },\n      (error) => {\n        console.log(error);\n      }\n    );\n  },\n  methods: {\n    moveWrite() {\n      this.$router.push({ name: \"NoticeWrite\" }); //이동..\n    },\n  },\n};\n</script>\n\n<style scope>\n.tdClass {\n  width: 50px;\n  text-align: center;\n}\n.tdSubject {\n  width: 300px;\n  text-align: left;\n}\n</style>\n"],"sourceRoot":"src/components/notice"}]}