QAAA,EAAA,YAAA;AACA,WAAA,WAAA;AACA;AAHA,GAxBA;;AA6BA,EAAA,OAAA,GAAA;AACA,IAAA,UAAA,CACA,MAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CADA,EAEA,QAAA,IAAA;AACA,WAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAJA,EAKA,KAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA;AACA,KAPA,CAAA;AASA,SAAA,WAAA,GAVA,CAWA;;AACA,SAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAZA,CAaA;;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,YAAA,KAAA,QAAA,EAAA;AACA,GA5CA;;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,WAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAHA;;AAIA,IAAA,WAAA,GAAA;AACA,MAAA,UAAA,CACA,MAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CADA,EAEA,QAAA,IAAA;AACA,aAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA,EAKA,KAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,cAAA,EAAA,KAAA;AACA,OAPA,CAAA;AASA,KAdA;;AAeA,IAAA,iBAAA,GAAA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,cADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,QAAA,EAAA,KAAA,MAAA,CAAA;AAAA;AAFA,OAAA,EADA,CAKA;AACA,KArBA;;AAsBA,IAAA,aAAA,GAAA;AACA,UAAA,OAAA,CAAA,SAAA,CAAA,EAAA;AACA,QAAA,aAAA,CAAA,KAAA,MAAA,CAAA,QAAA,EAAA,MAAA;AACA,eAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AACA,SAFA,CAAA;AAGA;AACA,KA5BA;;AA6BA,IAAA,eAAA,CAAA,OAAA,EAAA;AACA,WAAA,aAAA,GAAA,OAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAhCA;;AAiCA,IAAA,qBAAA,CAAA,MAAA,EAAA;AACA,WAAA,YAAA,GAAA,MAAA;AACA;;AAnCA;AA7CA,CAAA","sourcesContent":["<template>\n  <b-container class=\"bv-example-row mt-3\">\n    <b-row>\n      <b-col>\n        <b-alert show><h3>글보기</h3></b-alert>\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col class=\"text-left\">\n        <b-button variant=\"outline-primary\" @click=\"listArticle\">목록</b-button>\n      </b-col>\n      <b-col class=\"text-right\">\n        <b-button\n          variant=\"outline-info\"\n          size=\"sm\"\n          @click=\"moveModifyArticle\"\n          class=\"mr-2\"\n          >글수정</b-button\n        >\n        <b-button variant=\"outline-danger\" size=\"sm\" @click=\"deleteArticle\"\n          >글삭제</b-button\n        >\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col>\n        <b-card\n          :header-html=\"`<h3>${notice.noticeno}.\n          ${notice.subject} [${notice.hit}]</h3><div><h6>${notice.userid}</div><div>${notice.regtime}</h6></div>`\"\n          class=\"mb-2\"\n          border-variant=\"dark\"\n          no-body\n        >\n          <b-card-body class=\"text-left\">\n            <div v-html=\"message\"></div>\n          </b-card-body>\n        </b-card>\n      </b-col>\n    </b-row>\n    <!-- 도서정보 상세보기 Component -->\n    <comment-write :noticeno=\"this.noticeno\" />\n    <comment-write\n      v-if=\"isModifyShow && this.modifyComment != null\"\n      :modifyComment=\"this.modifyComment\"\n      @modify-comment-cancel=\"onModifyCommentCancel\"\n    />\n    <comment\n      v-for=\"(comment, index) in comments\"\n      :key=\"index\"\n      :comment=\"comment\"\n      @modify-comment=\"onModifyComment\"\n    />\n  </b-container>\n</template>\n\n<script>\nimport { getArticle, deleteArticle } from \"@/api/notice\";\nimport { getComment } from \"@/api/comment.js\";\nimport { mapGetters } from \"vuex\";\nimport CommentWrite from \"@/components/notice/comment/CommentWrite.vue\";\nimport Comment from \"@/components/notice/comment/Comment.vue\";\n\nexport default {\n  name: \"NoticeView\",\n  components: {\n    CommentWrite,\n    Comment,\n  },\n  data() {\n    return {\n      notice: {},\n      noticeno: \"\",\n      comments: [],\n      isModifyShow: false,\n      modifyComment: Object,\n    };\n  },\n  computed: {\n    message() {\n      if (this.notice.content)\n        return this.notice.content.split(\"\\n\").join(\"<br>\");\n      return \"\";\n    },\n\n    ...mapGetters([\"comments\"]),\n  },\n  watch: {\n    comments: function () {\n      this.listComment();\n    },\n  },\n  created() {\n    getArticle(\n      Number(this.$route.params.noticeno),\n      (response) => {\n        this.notice = response.data;\n      },\n      (error) => {\n        console.log(\"삭제시 에러발생!!\", error);\n      }\n    );\n    this.listComment();\n    // url이 query에서 noticeno 얻기.\n    this.noticeno = this.$route.query.noticeno;\n    // 도서평(댓글) 얻기.\n    this.$store.dispatch(\"getComments\", `/comment/${this.noticeno}`);\n  },\n  methods: {\n    listArticle() {\n      this.$router.push({ name: \"NoticeList\" });\n    },\n    listComment() {\n      getComment(\n        Number(this.$route.params.noticeno),\n        (response) => {\n          this.comments = response.data;\n        },\n        (error) => {\n          console.log(\"댓글 불러오기 에러! \", error);\n        }\n      );\n    },\n    moveModifyArticle() {\n      this.$router.replace({\n        name: \"NoticeUpdate\",\n        params: { noticeno: this.notice.noticeno },\n      });\n      //   this.$router.push({ path: `/board/modify/${this.article.articleno}` });\n    },\n    removeArticle() {\n      if (confirm(\"정말로 삭제?\")) {\n        deleteArticle(this.notice.noticeno, () => {\n          this.$router.push({ name: \"NoticeList\" });\n        });\n      }\n    },\n    onModifyComment(comment) {\n      this.modifyComment = comment;\n      this.isModifyShow = true;\n    },\n    onModifyCommentCancel(isShow) {\n      this.isModifyShow = isShow;\n    },\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components/notice"}]}